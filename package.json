{
  "name": "order-matching-backend",
  "version": "1.0.0",
  "description": "Order Matching System Backend (Node.js, TypeScript, Express, TypeORM, Redis, Socket.IO)",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:port": "PORT=3002 ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "npm run dev",
    "start:prod": "npm run build && npm run start"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.51.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.3.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.2",
    "@types/socket.io": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "nodejs",
    "express",
    "typeorm",
    "typescript",
    "backend",
    "socket.io",
    "redis",
    "order-matching"
  ],
  "author": "Seu Nome",
  "license": "MIT"
}
